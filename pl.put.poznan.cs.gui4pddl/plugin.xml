<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="pl.put.poznan.cs.gui4pddl.editor.PDDLEditor"
            default="false"
            extensions="pddl"
            icon="icons/pddl_file.gif"
            id="pl.put.poznan.cs.gui4pddl.PDDLEditor"
            name="PDDL Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="pl.put.poznan.cs.gui4pddl"
            name="GUI4PDDL">
      </category>
      <wizard
            category="pl.put.poznan.cs.gui4pddl"
            class="pl.put.poznan.cs.gui4pddl.ui.wizards.PDDLFileWizard"
            finalPerspective="pl.put.poznan.cs.gui4pddl.perspective.PDDLPerspective"
            icon="icons/pddl_file.gif"
            id="pl.put.poznan.cs.gui4pddl.ui.wizards.PDDLFileWizard"
            name="PDDL File"
            preferredPerspectives="pl.put.poznan.cs.gui4pddl.perspective.PDDLPerspective">
      </wizard>
      <wizard
            canFinishEarly="false"
            category="pl.put.poznan.cs.gui4pddl"
            class="pl.put.poznan.cs.gui4pddl.ui.wizards.PDDLProjectWizard"
            finalPerspective="pl.put.poznan.cs.gui4pddl.perspective.PDDLPerspective"
            hasPages="true"
            icon="icons/pddl_file.gif"
            id="pl.put.poznan.cs.gui4pddl.ui.wizards.PDDLProjectWizard"
            name="PDDL Project"
            preferredPerspectives="pl.put.poznan.cs.gui4pddl.perspective.PDDLPerspective"
            project="true">
      </wizard>
   </extension>
   <extension
         id="PDDLProject"
         name="PDDL Project"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="pl.put.poznan.cs.gui4pddl.ProjectNature1">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="pl.put.poznan.cs.gui4pddl.perspective.PDDLPerspectiveFactory"
            icon="icons/pddl_file.gif"
            id="pl.put.poznan.cs.gui4pddl.perspective.PDDLPerspective"
            name="PDDL">
      </perspective>
   </extension>

</plugin>
